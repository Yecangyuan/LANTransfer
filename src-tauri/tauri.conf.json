{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "productName": "局域网传输工具",
  "mainBinaryName": "lan-transfer",
  "version": "0.1.0",
  "identifier": "com.lantransfer.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "局域网传输工具",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}